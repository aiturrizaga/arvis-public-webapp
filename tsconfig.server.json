/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@arvis/*": [
        "app/*"
      ],
      "@arvis/env/*": [
        "environments/*"
      ],
      "@arvis/shared/*": [
        "app/shared/*"
      ],
      "@arvis/core/*": [
        "app/core/*"
      ]
    },
    "outDir": "./out-tsc/server",
    "types": [
      "node"
    ]
  },
  "files": [
    "src/main.server.ts",
    "server.ts"
  ]
}
