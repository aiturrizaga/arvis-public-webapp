/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@arvis/*": [
        "app/*"
      ],
      "@arvis/env/*": [
        "environments/*"
      ],
      "@arvis/shared/*": [
        "app/shared/*"
      ],
      "@arvis/core/*": [
        "app/core/*"
      ]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
